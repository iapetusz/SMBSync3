<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="AppUsedTheme" format="integer" />

    <style name="Main" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <item name="android:navigationBarColor">@color/navigationBarColorBlack</item>
        <item name="AppUsedTheme">0</item>
    </style>

    <style name="MainLight" parent="Theme.MaterialComponents.Light.DarkActionBar">
        <item name="android:textColor">@color/textColorLight</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="AppUsedTheme">1</item>
    </style>

    <style name="MainBlack" parent="Theme.MaterialComponents.NoActionBar">
        <item name="colorPrimary">@color/colorPrimaryBlack</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkBlack</item>
        <item name="android:windowBackground">@color/windowBackgroundBlack</item>
        <item name="android:navigationBarColor">@color/navigationBarColorBlack</item>
        <item name="AppUsedTheme">2</item>
    </style>
</resources>
